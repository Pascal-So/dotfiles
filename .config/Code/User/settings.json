{
    "workbench.colorTheme": "Bearded Theme feat. Melle Julie",
    "synthwave84.brightness": 1,
    "cmake.configureOnOpen": true,
    "cmake.buildDirectory": "${workspaceFolder}/build-${buildType}",
    "rust-client.engine": "rust-analyzer",
    "gitlens.codeLens.enabled": false,
    "rust.rust-analyzer.releaseTag": "stable",
    "rust.rust-analyzer.path": "rust-analyzer",
    "rust-client.revealOutputChannelOn": "warn",
    "gitlens.defaultGravatarsStyle": "retro",
    "editor.autoClosingBrackets": "never",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "html.autoClosingTags": false,
    "typescript.autoClosingTags": false,
    "javascript.autoClosingTags": false,
    "editor.autoSurround": "never",
    "editor.autoIndent": "brackets",
    "editor.unicodeHighlight.allowedCharacters": {
        "σ": true,
        "′": true
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "notebook.output.textLineLimit": 135,
    "editor.inlayHints.fontSize": 9,
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "rust-analyzer.server.path": null,
    "Lua.telemetry.enable": false,
    "github.copilot.advanced": {
        "debug.overrideEngine": "codegen",
        "debug.testOverrideProxyUrl": "http://localhost:5000",
        "debug.overrideProxyUrl": "http://localhost:5000"
    },
    "codeium.enableConfig": {
        "*": true,
        "rust": true,
        "php": true,
        "tex": false,
        "latex": false
    },
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "notebook.output.scrolling": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.inheritEnv": false,
    "codeium.enableCodeLens": false,
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingQuotes": "never",
    "diffEditor.ignoreTrimWhitespace": false,
    "jupyter.askForKernelRestart": false,
    "haskell.manageHLS": "PATH",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "html.autoCreateQuotes": false,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
